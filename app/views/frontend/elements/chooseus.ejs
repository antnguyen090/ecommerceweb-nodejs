<%
    let showItemsChoose = (data) =>{
        if(!data) return
        let dataParse = JSON.parse(data)
        let html=''
        dataParse.forEach(item =>{
            html += `
            <li class="d-flex justify-content-start mb-xl-7 mb-lg-5 mb-3">
                <span class="icon ${item.icon}"></span>
                <div class="alignLeft d-flex justify-content-start flex-wrap">
                    <h3 class="headingIII fwEbold mb-2">${item.title}</h3>
                    <p>${item.description}</p>
                </div>
            </li>
            `
        })
        return html
    }
%>
<!-- chooseUs-sec -->
<section class="chooseUs-sec container pt-xl-22 pt-lg-20 pt-md-16 pt-10 pb-xl-12 pb-md-7 pb-2">
  <div class="row">
      <div class="col-12 col-lg-6 mb-lg-0 mb-4">
            <img src="/uploads/setting/<%= settingPage.photochoose%>" alt="image description" class="img-fluid" onerror="this.onerror=null;this.src='/uploads/setting/no-image-found.jpg'">
      </div>
      <div class="col-12 col-lg-6 pr-4">
          <h2 class="headingII fwEbold playfair position-relative mb-6 pb-5"><%= settingPage.titleChoose%></h2>
          <p class="mb-xl-7 mb-lg-5"><%= settingPage.descriptionChoose%><a href="<%= settingPage.linkChoose%>" class="btnMore"><i>...Xem ThÃªm</i></a></p>
          <!-- chooseList -->
          <ul class="list-unstyled chooseList">
            <%- showItemsChoose(settingPage.list_choose)%>
          </ul>
      </div>
  </div>
</section>