<%- include('./../helpers/showmenubar'); -%>
		
		<!-- pageHeader -->
		<header id="header" class="position-absolute w-100">
			<div class="container-fluid px-xl-17 px-lg-5 px-md-3 px-0 d-flex flex-wrap">
				<div class="col-6 col-sm-3 col-lg-2 order-sm-2 order-md-0 dis-none">
					<!-- wishList -->
					<ul class="nav nav-tabs wishList pt-xl-5 pt-lg-4 pt-3 mr-xl-3 mr-0 justify-content-end border-bottom-0">
						<% if(checkLogin){ %>
							<% if(userInfo.group){ %>
										<% if(userInfo.group.group_acp === 'yes' && userInfo.group.status === 'active'){ %>
											<li class="nav-item"><a class="nav-link position-relative"  data-bs-custom-class="custom-tooltip" href="/admin" data-toggle="tooltip" data-placement="bottom" title="Trang Admin"><i class="fa-solid fa-users"></i></a></li>
											<% } %>
							<% } %>						
						<li class="nav-item"><a class="nav-link position-relative"  data-bs-custom-class="custom-tooltip" href="/trang-ca-nhan" data-toggle="tooltip" data-placement="bottom" title="Trang Cá Nhân"><i class="far fa-user-circle"></i></a></li>
						<li class="nav-item"><a class="nav-link position-relative"  data-bs-custom-class="custom-tooltip" href="/dang-xuat" data-toggle="tooltip" data-placement="bottom" title="Đăng Xuất"><i class="fa-solid fa-right-from-bracket"></i></a></li>
					 <% }else{ %>
						<li class="nav-item"><a class="nav-link position-relative"  data-bs-custom-class="custom-tooltip" href="/dang-nhap" data-toggle="tooltip" data-placement="bottom" title="Đăng Nhập"><i class="fa-solid fa-arrow-right-to-bracket"></i></a></li>
						<li class="nav-item"><a class="nav-link position-relative"  data-bs-custom-class="custom-tooltip" href="/dang-ky" data-toggle="tooltip" data-placement="bottom" title="Đăng Ký"><i class="far fa-registered mr-1"></i></a></li>
						<% } %>
						<li class="nav-item"><a class="nav-link position-relative"  data-bs-custom-class="custom-tooltip" href="/don-hang" data-toggle="tooltip" data-placement="bottom" title="Tra Cứu Đơn Hàng"><i class="fas fa-shipping-fast"></i></a></li>
					</ul>
				</div>
				<div class="col-12 col-sm-6 col-lg-8 static-block">
					<div class="nav-item logocenter">
						<a class="nLogo" href="/"><img src="/uploads/setting/<%=settingPage.logolarge%>" alt="Botanical" class="img-fluid"></a>
					</div>
					<!-- mainHolder -->
					<div class="mainHolder justify-content-center">
						<!-- pageNav1 -->
						<nav class="navbar navbar-expand-lg navbar-light p-0 pageNav1 position-static">
							<button type="button" class="navbar-toggle collapsed position-relative mt-md-2" data-toggle="collapse" data-target="#navbarNav" aria-expanded="false">
								<span class="sr-only">Toggle navigation</span>
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
							</button>
							<div class="collapse navbar-collapse" id="navbarNav">
								<ul class="navbar-nav mx-auto text-uppercase d-inline-block">
										<%- showMenuBar(listmenu, listCategory) %>
								</ul>
						</nav>
						<div class="logo">
							<a href="/"><img src="/uploads/setting/<%=settingPage.logosmall%>" alt="Botanical" class="img-fluid"></a>
						</div>
					</div>

				</div>
				<div class="col-6 col-sm-3 col-lg-2 order-sm-3 order-md-0 dis-none">
					<!-- wishList -->
					<ul class="nav nav-tabs wishList pt-xl-5 pt-lg-4 pt-3 mr-xl-3 mr-0 justify-content-end border-bottom-0">
						<li class="nav-item"><a class="nav-link icon-search search-btn" style="font-weight: 900;" href="javascript:void(0);" data-bs-custom-class="custom-tooltip" data-toggle="tooltip" data-placement="bottom" title="Tìm Kiếm"></a></li>
						<li class="nav-item"><a class="nav-link position-relative icon-heart" style="font-weight: 900;" href="/yeu-thich" data-bs-custom-class="custom-tooltip" data-toggle="tooltip" data-placement="bottom" title="Yêu Thích"><span class="num rounded d-block">1</span></a></li>
						<li class="nav-item"><a class="nav-link position-relative icon-cart" style="font-weight: 900;" href="/gio-hang" data-bs-custom-class="custom-tooltip" data-toggle="tooltip" data-placement="bottom" title="Giỏ Hàng" ><span class="num rounded d-block">2</span></a></li>
					</ul>
				</div>
			</div>
		</header>
	
	