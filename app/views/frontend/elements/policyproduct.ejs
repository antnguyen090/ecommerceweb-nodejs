<%
    let showPolicy = (data) =>{
        if(!data) return
        let html = ''
        let dataParse = JSON.parse(data)
        dataParse.forEach(item=>{
            html += `
            <div class="col-12 col-sm-6 col-lg-3 mb-4 mb-lg-0">
                <!-- contactListColumn -->
                <div class="contactListColumn border overflow-hidden py-xl-5 py-md-3 py-2 px-xl-6 px-md-3 px-3 d-flex">
                    <span class="icon ${item.icon}"></span>
                    <div class="alignLeft pl-2">
                        <strong class="headingV fwEbold d-block mb-1">${item.title}</strong>
                        <p class="m-0">${item.description}</p>
                    </div>
                </div>
            </div>
            `
        })
        return html
    }
%>

<!-- contactListBlock -->
<div class="contactListBlock container overflow-hidden pt-xl-4 pt-lg-5 pt-md-4 pt-2 pb-xl-6 pb-lg-5 pb-md-2 pb-1">
  <div class="row">
      <%- showPolicy(settingPage.list_policy) %>
  </div>
</div>