<%
  showDeliveryList = (data, current) =>{
    if(!data) return
    let currentId
    let select = ''
    if(!current) {
      select = 'selected'
    } else{
      currentId = current.toString()
    }
    let html=`<option ${select} >Chọn Tỉnh Của Bạn</option>`
    data.forEach(item=>{
      select = (currentId == item.id) ? 'selected' : ''
      html += `
      <option value="${item.id}" ${select} >${item.province}</option>
      `
    })
    return html
  }
%>