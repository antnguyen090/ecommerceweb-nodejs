<% 
	const linkThumbSetting = "/" + "uploads" + `/setting/`;
%>
<!-- introBannerHolder -->
<section class="introBannerHolder d-flex w-100 bgCover" style="background-image: url(<%= linkThumbSetting + settingPage.logobanner%>);">
  <div class="container">
      <div class="row">
          <div class="col-12 pt-lg-23 pt-md-15 pt-sm-10 pt-6 text-center">
              <ul class="list-unstyled breadCrumbs d-flex justify-content-center" style="margin-top: 12%; font-size: 15px;"> 
                  <li class="mr-2"><a href="/">Trang Chủ</a></li>
                  <li class="mr-2">/</li>
                  <li class="active"><%= pageTitle %></li>
              </ul>
          </div>
      </div>
  </div>
</section>
<section class="container  pb-xl-6 pb-lg-5 pb-md-4  pb-2">
  <div class="row">
      <div class="col-12 registerSec">
          <div class="card" style="border-radius: 15px; margin-top: 2px;">
              <div class="card-body p-5 trackingForm">
              <h4 class="text-uppercase text-center mb-5">Tra Cứu Đơn Hàng</h4>
                  <form id="formFindOrder">
                    <div class="form-outline mb-2">
                      <input type="text"  name="codeOrder" class="form-control form-control-lg" required/>
                      <label class="form-label" for="form3Example4cdg">Nhập mã đơn hàng</label>
                    </div>
                    <div class="d-flex justify-content-center spinner">
                      <button type="submit"
                        class="btn btn-success btn-block btn-lg gradient-custom-4 ">Tra Cứu</button>
                    </div>
                  </form>
              </div>

          </div>
      </div>
  </div>
</section>