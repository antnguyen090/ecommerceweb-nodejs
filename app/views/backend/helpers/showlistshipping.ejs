<%
  showListShipping = (data) =>{
        let dataParse = (data == undefined) ? [{title:"", desciption:""}] : JSON.parse(data)
        let html = ""
        dataParse.forEach((item, index) =>{
            html+= `
            <div id="shipping-dynamic-field-${index+1}" class="form-group shipping-dynamic-field">
              <label for="field" class="font-weight-bold">Step ${index+1}</label>
              <div class="row">
                <div class="col-sm-2">
                    <label for="title_shipping_1[]" class="col-form-label text-sm-right">Title</label>
                </div>
                <div class="col-sm-8">
                    <input type="text" class="form-control" name="title_shipping_1[]" value="${item.title}"/>
                </div>
              </div>
              <div class="row mt-2">
                <div class="col-sm-2">
                    <label for="description_shipping_1[]" class="col-form-label text-sm-right">Description</label>
                </div>
                <div class="col-sm-8">
                    <input type="text" class="form-control" name="description_shipping_1[]" value="${item.description}"/>
                </div>
              </div>
            </div>
             `
        })
        return html
  }

%>